@model ResponsibleRegisterViewModel

<div class="container">
    <div class=" pt-85 pb-90">
        <div class="container">
            <div class="row">
                <div class="col-lg-7 col-md-12 ml-auto mr-auto">
                    <div class="register-wrapper">
                        <div class="register-tab-list nav">
                            <a class="active">
                                <h4>  ورود کارشناسان </h4>
                            </a>
                        </div>

                        <div class="tab-content register-area" dir="rtl" style="text-align: start">
                             
                            <div class="tab-pane active " style="">

                                <div class="login-form-container rounded-xl">

                                    <div class="register-form">

                                        <form>

                                            <div class="text-danger"></div>

                                            <div class="form-group">
                                                <label asp-for="ResponsibleId" class="control-label"></label>
                                                <input type="text" id="ResponsibleIdInput" class="form-control" />

                                                <span id="ResponsibleIdCheck" class="text-danger"></span>

                                            </div>
                                            <div class="form-group">
                                                <label asp-for="NationalNo" class="control-label"></label>
                                                <input type="text" id="ResponsibleNoInput" class="form-control" />

                                                <span id="ResponsibleNationalNoCheck" class="text-danger"></span>

                                            </div>

                                            <a id="ResponsibleLogin" class="btn btn-block btn-lg btn-magenta shadow-lg text-white">ورود</a>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts{

    <link href="~/Sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <script src="~/Sweetalert2/sweetalert2.min.js"></script>
    <script src="~/js/Index-min.js"></script>

}